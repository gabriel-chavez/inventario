@model sistemainventario.Models.enlaces

@{
    //WebApplication2.Models.PessoaAplic Pessoa = (WebApplication2.Models.PessoaAplic)ViewBag.DadosPessoa;

    var oficinas = ViewBag.oficinas;
    var proveedores = ViewBag.proveedores;
    var enlacesTipo = ViewBag.enlacesTipo;
    var enlacesTecnologia = ViewBag.enlacesTecnologia;
    List<sistemainventario.Models.enlaces> enlacesNacional = ViewBag.nacional;
    List<sistemainventario.Models.enlaces> enlacesLocal = ViewBag.local;
    List<sistemainventario.Models.enlaces> enlacesServicios = ViewBag.servicios;
    List<sistemainventario.Models.enlaces> enlacesInternet = ViewBag.Internet;
    //sistemainventario.Models.enlaces modeloenlaces = sistemainventario.Models.enlaces;
}
@*@Html.TextBoxFor(x => x.enlace, new { @class = "form-control" })*@

<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3>
                Comunicaciones <small> Enlaces </small>
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                @*<div class="x_title">
                    <h2><b>Enlaces </b><small></small></h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                        
