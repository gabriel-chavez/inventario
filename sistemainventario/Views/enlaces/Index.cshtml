}
@*@Html.TextBoxFor(x => x.enlace, new { @class = "form-control" })*@

<div class="">
    <div class="page-title">
        <div class="title_left">
            <h3>
                Comunicaciones <small> Enlaces </small>
            </h3>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <div class="x_panel">
                @*<div class="x_title">
