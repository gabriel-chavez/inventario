                <div class="x_content">
                    <!-- content starts here -->
                    <div>
                        <!-- Nav tabs -->
                        <ul class="nav nav-tabs" role="tablist">
                            <li role="presentation" class="active"><a href="#nacional" aria-controls="home" role="tab" data-toggle="tab">Enlace Nacional</a></li>
                            <li role="presentation"><a href="#local" aria-controls="profile" role="tab" data-toggle="tab">Enlaces Local</a></li>
                            <li role="presentation"><a href="#servicio" aria-controls="messages" role="tab" data-toggle="tab">Enlaces Servicio</a></li>
                            <li role="presentation"><a href="#internet" aria-controls="settings" role="tab" data-toggle="tab">Enlaces Internet</a></li>
                        </ul>

                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="nacional">
                                <!--Nacional-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesNacional },
                        {"titulo","Nacional" }
                })
                            </div>
                            <div role="tabpanel" class="tab-pane" id="local">
                                <!--Local-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesLocal },
                        {"titulo","Local" }
                })

                            </div>
                            <div role="tabpanel" class="tab-pane" id="servicio">
                                <!--Servicio-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesServicios },
                        {"titulo","Servicios" }
                })
                            </div>
                            <div role="tabpanel" class="tab-pane" id="internet">
                                <!--Internet-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesInternet },
                        {"titulo","Internet" }
                })
                            </div>
                        </div>

                    </div>

                    <div class="clearfix"></div>
                    @{      /*Request.Url.GetLeftPart(UriPartial.Authority) + Request.ApplicationPath*/
                    }
                </div>
            </div>
        </div>
    </div>
</div> 
