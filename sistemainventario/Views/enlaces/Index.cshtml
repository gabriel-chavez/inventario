                            <li role="presentation"><a href="#internet" aria-controls="settings" role="tab" data-toggle="tab">Enlaces Internet</a></li>
                        </ul>

                        <!-- Tab panes -->
                        <div class="tab-content">
                            <div role="tabpanel" class="tab-pane active" id="nacional">
                                <!--Nacional-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesNacional },
                        { "oficinas", oficinas },
                        { "proveedores", proveedores },
                        { "enlacesTipo", enlacesTipo },
                        { "enlacesTecnologia", enlacesTecnologia },
                        {"titulo","Nacional" }
                })
                            </div>
                            <div role="tabpanel" class="tab-pane" id="local">
                                <!--Local-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesLocal },
                        { "oficinas", oficinas },
                        { "proveedores", proveedores },
                        { "enlacesTipo", enlacesTipo },
                        { "enlacesTecnologia", enlacesTecnologia },
                        {"titulo","Local" }
                })

                            </div>
                            <div role="tabpanel" class="tab-pane" id="servicio">
                                <!--Servicio-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesServicios },
                        { "oficinas", oficinas },
                        { "proveedores", proveedores },
                        { "enlacesTipo", enlacesTipo },
                        { "enlacesTecnologia", enlacesTecnologia },
                        {"titulo","Servicio" }
                })
                            </div>
                            <div role="tabpanel" class="tab-pane" id="internet">
                                <!--Internet-->
                                @Html.Partial("tabla/tabla", new ViewDataDictionary {
                        { "datos", enlacesInternet },
                        { "oficinas", oficinas },
                        { "proveedores", proveedores },
                        { "enlacesTipo", enlacesTipo },
                        { "enlacesTecnologia", enlacesTecnologia },
                        {"titulo","Internet" }
                })
                            </div>
                        </div>

                    </div>

                    <div class="clearfix"></div>
                    @{      /*Request.Url.GetLeftPart(UriPartial.Authority) + Request.ApplicationPath*/
                    }
                </div>
            </div>
        </div>
