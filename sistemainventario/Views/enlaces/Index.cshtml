    var oficinas = ViewBag.oficinas;
    var proveedores = ViewBag.proveedores;
    var enlacesTipo = ViewBag.enlacesTipo;
    var enlacesTecnologia = ViewBag.enlacesTecnologia;
    List<sistemainventario.Models.enlaces> enlacesNacional = ViewBag.nacional;
    List<sistemainventario.Models.enlaces> enlacesLocal = ViewBag.local;
    List<sistemainventario.Models.enlaces> enlacesServicios = ViewBag.servicios;
    List<sistemainventario.Models.enlaces> enlacesInternet = ViewBag.Internet;
